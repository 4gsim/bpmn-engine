    {
      "type": "MIT",
      "url": "https://github.com/paed01/bpmn-engine/master/LICENSE"
    }
  ],
  "nyc": {
    "exclude": [
      "dist",
      "test",
      "rollup.config.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "bent": "^1.5.13",
    "bpmn-moddle": "^5.2.0",
    "camunda-bpmn-moddle": "^3.2.0",
