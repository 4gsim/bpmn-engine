    "rollup-plugin-license": "^0.9.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "unnecessary": "^1.3.3"
  },
  "dependencies": {
    "bpmn-elements": "^0.2.0",
    "debug": "^4.1.1",
    "moddle-context-serializer": "^0.4.0",
    "smqp": "^1.6.0"
